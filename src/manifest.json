{
    "name": "Google Calendar links to ICS",
    "description": "Intercepts links that create new Google Calendar events and turns them into iCalendar files",
    "manifest_version": 2,
    "version": "1.1",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://calendar.google.com/calendar/render*",
        "https://calendar.google.com/calendar/r/eventedit*",
        "https://calendar.google.com/calendar/event*"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "32": "icon-32.png",
        "64": "icon-64.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{3aead9aa-6e1a-4a52-aa0b-ebc920ffef74}",
            "data_collection_permissions": {
                "required": ["none"]
            }
        }
    }
}
